1094

import java.util.*;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int[]arr = new int[23];
        int N = sc.nextInt();
        for(int i=0; i<N; i++){
                arr[sc.nextInt()-1]++;
        }
        
        for(int i=0; i<arr.length; i++)System.out.print(arr[i] + " ");
    }
}
